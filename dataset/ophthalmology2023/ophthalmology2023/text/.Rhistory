# 加载所需包
library(officer)
# 批量生成文档
for (i in 1:60) {
# 格式化数字为三位数（001-075）
doc_number <- sprintf("%03d", i)
# 创建文件名
filename <- paste0("ophthalmology", doc_number, ".docx")
# 创建空白Word文档
doc <- read_docx()
# 在文档中添加一个空段落（生成有效文档结构）
doc <- body_add_par(doc, "")
# 保存文档
print(doc, target = filename)
}
# 完成提示
cat("已成功生成60个空白Word文档")
